<!-- views/room.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>

</head>
<body class="container">

<header>
    
</header>

<main>
    <div class="jumbotron">
        <h1><%= building + " " + room %></h1>
        <ul>
        <% machines.forEach((machine) => { %>
            <!--
                List of all machines for that room
            -->
            <li><a href="/<%= building %>/<%= room %>/<%= machine.name %>"><%= machine.name %></a></li>   
        <% }) %>
        </ul>
        
        <iframe src="https://calendar.google.com/calendar/embed?mode=WEEK&amp;height=600&amp;wkst=1&amp;bgcolor=%23FFFFFF&amp;src=<%= machines[0].google_calender_id %>&amp;color=%23182C57&amp;ctz=America%2FNew_York" 
        style="border-width:0" width="800" height="600" frameborder="0" scrolling="no"></iframe>

        <img src="https://www.cs.purdue.edu/cams-delayed/<%= building + room %>.jpg"></img>

    </div>
</main>

<footer>
</footer>

</body>
</html>





